{
    "expo": {
      "name": "Aura Shield: Real-Time Threat Monitoring for VIPs",
      "slug": "aura-shield-real-time-threat-monitoring-for-vips",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "myapp",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": false,
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "app.rork.aura-shield-real-time-threat-monitoring-for-vips"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        },
        "package": "app.rork.aura-shield-real-time-threat-monitoring-for-vips",
        "permissions": [
          "INTERNET",
          "ACCESS_NETWORK_STATE"
        ]
      },
      "web": {
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        [
          "expo-router",
          {
            "origin": "https://rork.com/"
          }
        ],
        "expo-localization"
      ],
      "experiments": {
        "typedRoutes": true
      },
      "extra": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "${EXPO_PUBLIC_GEMINI_API_KEY}"
      }
    }
  }